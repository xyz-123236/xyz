<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.xyz.mapper.UserMapper">

    <select id="selectAll" resultType="cn.xyz.pojo.User">
        select * from user ${ew.customSqlSegment}
    </select>


    <select id="get" parameterType="cn.xyz.pojo.User" resultType="cn.xyz.pojo.User">
        select * from user where age > #{age}
    </select>

    <select id="findPage" resultType="cn.xyz.pojo.User">
        select * from user ${ew.customSqlSegment}
    </select>
</mapper>